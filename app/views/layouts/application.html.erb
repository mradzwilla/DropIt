<!DOCTYPE html>
<html>
<head>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if user_signed_in? %>
	<p>Hello, <%= current_user.first_name %> - <%= link_to "Sign Out", destroy_user_session_path, :method => "delete" %>
<% end %>
<a href='#' id="sign_in">Sign in with Facebook SDK</a>
<!-- <a href='#' id="sign_out">Sign out with Facebook SDK</a> -->
<%= yield %>

</body>
</html>
